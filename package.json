{
  "name": "component-fixture",
  "version": "0.4.1",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js --open",
    "build:doc": "vue-cli-service build --modern --entry ./example/main.js --dest docs",
    "build": "vue-cli-service build --name ComponentFixture --entry ./src/index.js --target lib",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit --coverage",
    "test:coverage": "vue-cli-service test:unit --coverage --verbose && codecov",
    "prepublishOnly": "npm run lint && npm run test:unit && npm run build:doc && npm run build",
    "doc:build": "npx vuedoc.md --section API --output ./README.md ./src/components/ComponentFixture.vue"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "font-awesome": "^4.7.0",
    "v-tooltip": "^2.0.0-rc.33",
    "vue": "^2.6.9",
    "vue-splitpane": "^1.0.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-plugin-unit-jest": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-prettier": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "@vuedoc/md": "^1.5.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "circular-json": "^0.5.9",
    "codecov": "^3.1.0",
    "epic-spinners": "^1.0.4",
    "jquery": "^3.3.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "popper.js": "^1.14.4",
    "vue-cli-plugin-component": "^1.7.1",
    "vue-cute-rate": "^1.0.2",
    "vue-d3-sunburst": "^0.1.10",
    "vue-router": "^3.0.2",
    "vue-slide-bar": "^1.1.9",
    "vue-svg-gauge": "^1.0.0",
    "vue-template-compiler": "^2.6.9",
    "vue-typer": "^1.2.0",
    "vued3tree": "^3.7.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/components/ComponentFixture.vue",
      "<rootDir>/src/utils/**"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "main": "dist/ComponentFixture.umd.js",
  "module": "dist/ComponentFixture.common.min.js",
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "keywords": [
    "vue",
    "component"
  ],
  "license": "MIT"
}
